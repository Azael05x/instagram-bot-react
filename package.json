{
  "name": "hot-module-reloading",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --progress --color --config webpack/webpack.config.development.js --watch -d",
    "docker": "webpack-dev-server  --progress --color --config webpack/webpack.config.development.js --host=0.0.0.0",
    "prebuild": "rimraf ./dist/*",
    "build": "webpack --color --config webpack/webpack.config.production.js",
    "test:watch": "jest --watchAll",
    "test": "jest --ci --runInBand"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internal/tests/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/internal/tests/styleMock.js"
    }
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^21.0.0",
    "@types/lodash": "^4.14.104",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.12",
    "@types/react-router-dom": "^4.0.5",
    "@types/react-test-renderer": "^16.0.0",
    "@types/react-transition-group": "^2.0.2",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.17.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "fork-ts-checker-notifier-webpack-plugin": "^0.2.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.1.0",
    "jest-junit": "^3.1.0",
    "node-sass": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.19.1",
    "ts-jest": "^21.0.1",
    "ts-loader": "^3.0.0",
    "tslib": "^1.7.1",
    "tslint": "^5.5.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "lodash": "^4.17.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
